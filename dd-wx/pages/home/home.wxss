page {
  
  width: 100%;
 height: 100%;
   background: #F8F8F8;

}
.contain{
  width: 100%;
  min-height: 100%;
  background: #F8F8F8;
  height: 100%

}
.include{
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #fff
}
.top{
  width: 100%;
  height: 80rpx;
  border-bottom: 1px solid  rgba(25, 31, 37, 0.12);
  position: relative
}
.top>.img1{
  width: 104rpx;
  height: 46rpx;
  float: right;
  vertical-align: middle;

  line-height: 56rpx;
  margin-right: 34rpx;
}
.top>.img2{
  width: 48rpx;
  height: 48rpx;
  float: right;
  vertical-align: middle;
  margin-right: 32rpx;
  line-height: 48rpx
}
.list{
  width: 100%;
  height: auto;
  overflow: hidden
}
.item{
  width: 100%;
  height: 147rpx;
  display: flex;
  justify-content: space-between
}
.item>.left{
  width: 15%;
  height: 100%;
  display: inline-block
}
.item>.right{
    width: 82%;
  height: 100%;
    display: inline-block;
    display: flex;
    justify-content: space-between;
  border-bottom: 1px solid  rgba(25, 31, 37, 0.12);

}
.item>.left>image{
  width: 80rpx;
  height: 80rpx;
  float: right;
  line-height: 140rpx;
  vertical-align: middle;
  margin-top: 28rpx
  
}
.item>.right>.leftin>text{
 display: block;
 
font-size: 28rpx;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: 500;
color: #191F25;
margin-top: 30rpx

}
.item>.right>.leftin>.obj{
  margin-top: 10rpx;
  display: flex;
  align-items: center;


}
.item>.right>.leftin>.obj>text{


  
font-size: 20rpx;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: 500;
color: #5A5A5A;

}
.item>.right>.leftin>.obj>button{
  width: 154rpx;
height: 40rpx;
border-radius: 4rpx;
border: 1px solid #3296FA;
display: inline-block;
background: none;
font-size: 20rpx;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: 500;
color: #3296FA;
margin-left: 32rpx;
line-height: 40rpx
}
.item>.right>.rightin{
  display: flex;
  align-items: center;
  position: relative
  
}
.item>.right>.rightin>image{
  width: 48rpx;
  height: 48rpx;
  vertical-align: middle;
  margin-right: 32rpx;
  line-height: 140rpx;
 margin-top: 34rpx
}
.foot{
  margin-top: 32rpx;
  text-align: center
}
.foot>text{
  
font-size: 24rpx;
font-family: PingFangSC-Regular, PingFang SC;
font-weight: 400;
color: #3296FA;
}
.edit{
position: absolute;
width: 218rpx;
height: 206rpx;
background: #FFFFFF;
box-shadow: 0px 4rpx 12rpx 0px rgba(0, 0, 0, 0.2);
right: 20rpx;
top: 20rpx
}
.edit>text{
  display: block;
 width: 218rpx;
height: 103rpx;
text-align: center;
line-height: 96rpx;

border-bottom: 1px solid #F0F0F0;
}