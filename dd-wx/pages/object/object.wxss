page{
  width: 100%;
  height: 100%;
     background: #F8F8F8;
}
.contain{
    width: 100%;
  height: 100%;

}
.top{
  background: #fff;
  width: 100%;
  height: 90rpx;
     border-bottom: 1px solid #F8F8F8;
}
.top>image{
  width: 104rpx;
  height: 46rpx;
  float: right;
  vertical-align: middle;

  line-height: 56rpx;
  margin-right: 34rpx;
}
.list{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.item{
  width: 100%;
  height: 98rpx;
  background: #fff;
  margin-bottom: 12rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  line-height: 98rpx
}
.item>.left{
  width: 75%;
}
.item>.left>text{
  
font-size: 28rpx;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: 500;
color: #191F25;
}
.item>.right>.del{
  
font-size: 24rpx;
font-family: PingFangSC-Regular, PingFang SC;
font-weight: 400;
color: #6A6A6A;
z-index: 9999999;
position: relative;

}
.item>.right>.edit{
  
font-size: 24rpx;
font-family: PingFangSC-Regular, PingFang SC;
font-weight: 400;
color: #3296FA;
margin-left: 32rpx;
z-index: 9999999;
position: relative;
}