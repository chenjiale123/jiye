page{
  width: 100%;
  height: 100%;
     background: #F8F8F8;
}
.contain{
    width: 100%;
  height: 100%;

}
.top{
  background: #fff;
  width: 100%;
  height: 90rpx;
     border-bottom: 1px solid #F8F8F8;
}
.top>text{
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #191F25;
display: block;
  margin-right: 30rpx;
  margin-top: 20rpx;
  font-weight: 500;
  float: right;
}
.list{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.item{
  width: 100%;
  height: 98rpx;
  background: #fff;
  margin-bottom: 12rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  line-height: 98rpx;
  align-items: center;
}

.item>.left>text{
  
font-size: 28rpx;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: 500;
color: #191F25;
}
.item>.right>.del{
   display: block;
  width: 92rpx;
  height: 36rpx;
  background: #CCE5FF;
  border-radius: 2px;
  
font-size: 24rpx;
font-family: PingFangSC-Regular, PingFang SC;
font-weight: 400;
color: #3296FA;
text-align: center;
line-height: 36rpx;
vertical-align: middle;
}
.item>.right>.del1{
  display: block;
 width: 92rpx;
 height: 36rpx;
 background: rgb(255, 204, 204);
